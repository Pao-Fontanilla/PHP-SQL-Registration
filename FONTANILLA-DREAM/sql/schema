create table user_records (
	id INT AUTO_INCREMENT PRIMARY KEY,
	first_name VARCHAR(50),
	last_name VARCHAR(50),
	email VARCHAR(50),
	gender VARCHAR(50),
	age INT,
	job_position VARCHAR(50),
	location VARCHAR(50),
	date_added TIMESTAMP DEFAULT CURRENT_TIMESTAMP
	
);
